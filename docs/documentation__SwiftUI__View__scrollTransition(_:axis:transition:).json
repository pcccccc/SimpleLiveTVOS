      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "type": "text",
                  "text": "The configuration controlling how the"
                },
                {
                  "type": "text",
                  "text": " "
                },
                {
                  "type": "text",
                  "text": "transition will be applied. The configuration will be applied both"
                },
                {
                  "type": "text",
                  "text": " "
                },
                {
                  "type": "text",
                  "text": "while the view is coming into view and while it is disappearing (the"
                },
                {
                  "type": "text",
                  "text": " "
                },
                {
                  "type": "text",
                  "text": "transition is symmetrical)."
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "configuration"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The axis of the containing scroll view over which the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "transition will be applied. The default value of ",
                  "type": "text"
                },
                {
                  "code": "nil",
                  "type": "codeVoice"
                },
                {
                  "text": " uses the",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "axis of the innermost containing scroll view, or ",
                  "type": "text"
                },
                {
                  "code": ".vertical",
                  "type": "codeVoice"
                },
                {
                  "text": " if",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "the innermost scroll view is scrollable along both axes.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "axis"
        },
        {
          "content": [
            {
              "type": "paragraph",
              "inlineContent": [
                {
                  "type": "text",
                  "text": "A closure that applies visual effects as a function of"
                },
                {
                  "type": "text",
                  "text": " "
                },
                {
                  "type": "text",
                  "text": "the provided phase."
                }
              ]
            }
          ],
          "name": "transition"
        }
      ]
    }
